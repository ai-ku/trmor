#!/usr/local/bin/xfst -f

clear

read lexc nzero.lexc
upper net
define nzero
define model [ "+Adj" "^DB" "+Noun" "+Zero" nzero ];

load tfnowords.fst
define tf

read regex [ model - tf ];
print words > nzero.extra
pop

read regex [ ?+ "^DB" "+Noun" "+Zero" \"^DB"+ ];
load Milliyet.fst
intersect net
define test0

read regex [ ?+ "^DB" "+Noun" "+Zero" @-> 0 ];
read regex [ "+Prop" -> 0 ];
push test0
compose net
lower net
define test

read regex [ test - nzero ];
print words > nzero.missing
pop
